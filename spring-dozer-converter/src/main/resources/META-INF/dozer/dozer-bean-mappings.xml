
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net http://dozer.sourceforge.net/schema/beanmapping.xsd">

	<mapping>
		<class-a>com.rakeshjavahub.pojo.Person</class-a>
		<class-b>com.rakeshjavahub.schema.beans.Person</class-b>
		<field>
			<a>email</a>
			<b>emailAddress</b>
		</field>
		<field-exclude>
			<a>address</a>
			<b>address</b>
		</field-exclude>
	</mapping>
</mappings>